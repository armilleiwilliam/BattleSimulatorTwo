{% extends 'base.html.twig' %}

{% block body %}
    <h1>Typesofcombatant</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ typesOfCombatant.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ typesOfCombatant.name }}</td>
            </tr>
            <tr>
                <th>Createdat</th>
                <td>{% if typesOfCombatant.createdAt %}{{ typesOfCombatant.createdAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if typesOfCombatant.updatedAt %}{{ typesOfCombatant.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('combatants_typesofcombatants_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('combatants_typesofcombatants_edit', { 'id': typesOfCombatant.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
